{
  "expo": {
    "name": "aquarium",
    "slug": "aquarium",
    "version": "1.0.1",
    "description": "Just a simple aquarium. Nothing extra",
    "scheme": "aquarium",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1886f3"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1886f3"
      },
      "package": "com.aquarium.simpleaquarium"
    },
    "extra": {
      "eas": {
        "projectId": "ec88f5da-fcc3-44cb-a0f7-92de91b104fc"
      }
    },
    "owner": "aquarium",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ec88f5da-fcc3-44cb-a0f7-92de91b104fc"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "usesCleartextTraffic": true
          }
        }
      ]
    ]
  }
}
